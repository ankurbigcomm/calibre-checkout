            <stringProp name="script">def sessionId = vars.get(&quot;shopSessionId&quot;)
def expiryDate = vars.get(&quot;sessionExpiry&quot;)
def formattedExpiryDate = Date.parse(&quot;EEE, dd MMM yyyy HH:mm:ss zzz&quot;, expiryDate).format(&quot;yyyy-MM-dd HH:mm:ss.SSS&apos;Z&apos;&quot;)

File file = new File(&quot;cookie-jar.txt&quot;)
file.append(&quot;# Netscape HTTP Cookie File\n&quot;)
file.append(&quot;# http://curl.haxx.se/rfc/cookie_spec.html\n&quot;)
file.append(&quot;.storefront-control.mybigcommerce.com\tTRUE\t/\tTRUE\t&quot; + formattedExpiryDate + &quot;\tSHOP_SESSION_TOKEN\t&quot; + sessionId)
println file.text
</stringProp>
